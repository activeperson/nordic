<script type="text/javascript">
  $(".js-lang-title:contains('eesti')").empty().append('<div class="flag-wrapper"><div class="flags et"></div></div>');
  $(".js-lang-title:contains('English')").empty().append('<div class="flag-wrapper"><div class="flags uk"></div></div>');
  $(".js-lang-title:contains('Norsk')").empty().append('<div class="flag-wrapper"><div class="flags no"></div></div>');
  $(".js-lang-title:contains('Deutsch')").empty().append('<div class="flag-wrapper"><div class="flags de"></div></div>');
  $(".js-lang-title:contains('Swedish')").empty().append('<div class="flag-wrapper"><div class="flags sv"></div></div>');
  $(function() {
    $(".js-lang-title").css({
      visibility: 'visible',
    });
  });
</script>


 



<script type="text/javascript">

    (function($) {
        if(window.location.href.indexOf("?") > -1) {
            var openBlock = window.location.href.split('?')[1];
            openBlock = decodeURIComponent(openBlock);
            var blockButton = $('.js-anchored-heading[data-anchor="'+ openBlock +'"]').closest('.single-content-block').find('.js-show-more');
            contentRevealOnClick(blockButton);
            setTimeout(
                function() {
                    afterScrollShowCorrectAncour(openBlock);
                }, 1000);
        }

        $('body').on('click', '.js-show-more', function() {
            contentRevealOnClick($(this));
        });

        if ($(window).width() <= 768) {
            $('.edy-texteditor-container.image-container.edy-positionable-container-center').each(function(i, imageContainer){
                if ($(imageContainer).height() > 120) {
                    $(imageContainer).css("width", 101);
                }
            });
        }

        $(window).on('resize', function(){
            if ($(window).width() <= 768) {
                $('.edy-texteditor-container.image-container.edy-positionable-container-center').each(function(i, imageContainer){
                    if ($(imageContainer).height() > 120) {
                        $(imageContainer).css("width", 101);
                    }
                });
            }
        });

        $('body').on('click', '.js-show-more.open', function() {
            var revealedContent = $(this).closest('.single-content-block').find('.js-hidden-full-content');
            $(revealedContent).css('max-height' , '0');
            //$('body').animate({scrollTop: $(this).closest('.single-content-block').find('.main-heading').offset().top}, 500);
            $(this).closest('.single-content-block').find('.main-heading')
              .velocity('stop')
              .velocity('scroll', { duration: 500 });
        });

    })(jQuery);


    function contentRevealOnClick(clickedButton) {
        var revealedContent = clickedButton.closest('.single-content-block').find('.js-hidden-full-content');
        var revealedContentInner = clickedButton.closest('.single-content-block').find('.js-hidden-full-content-inner').height();
        clickedButton.prop('disabled', true);
        $(revealedContent).css('max-height' , revealedContentInner);
        $(revealedContent).toggleClass('active');
        $(revealedContent).delay(2000).queue(function (next) {$(revealedContent).css('max-height', $(revealedContent).height()); next(); $('.js-show-more').prop('disabled', false);});
        clickedButton.toggleClass('open');
    }

    function afterScrollShowCorrectAncour(openBlock) {
        $('.sidebar-option-inner').removeClass('active');
        $('.js-sidebar-option[data-anchor="'+ openBlock +'"]').find('.sidebar-option-inner').addClass('active');
    }

</script>
