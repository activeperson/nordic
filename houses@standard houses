<script type="text/javascript">
  $(".js-lang-title:contains('eesti')").empty().append('<div class="flag-wrapper"><div class="flags et"></div></div>');
  $(".js-lang-title:contains('English')").empty().append('<div class="flag-wrapper"><div class="flags uk"></div></div>');
  $(".js-lang-title:contains('Norsk')").empty().append('<div class="flag-wrapper"><div class="flags no"></div></div>');
  $(".js-lang-title:contains('Deutsch')").empty().append('<div class="flag-wrapper"><div class="flags de"></div></div>');
  $(".js-lang-title:contains('Swedish')").empty().append('<div class="flag-wrapper"><div class="flags sv"></div></div>');
  $(function() {
    $(".js-lang-title").css({
      visibility: 'visible',
    });
  });
</script>


 


    
    
    
    
    <script type="text/javascript">
        $('.js-details-item-second').each(function(i, item) {
            $(item).text($(item).text().replace(/ *\([^)]*\) */g, ""));
        });
        
        var titleHeight = 0;
        $('.houses-group').each(function(i, group) {
            var houseTitles = [];
            var housesCount = $('.js-single-house').length;
            $('.js-single-house').each(function(j, house) {
                houseTitles.push($(house).find('.house-title'));
                var currentTitleHeight = $(house).find('.house-title').height();
                if (titleHeight < currentTitleHeight) {
                    titleHeight = currentTitleHeight;
                }
                
                if (j % 3 === 2 || j === housesCount - 1) {
                    for (var i = houseTitles.length - 1; i >= 0; i--) {
                        var $housetitle = houseTitles[i];
                        $housetitle.height(titleHeight);
                    }
                    houseTitles = [];
                    titleHeight = 0;
                }
            });
        });
        
        $('.js-single-house').matchHeight();
    </script>
    
    
    <script type="text/javascript">
        // Remove empty values
        Array.prototype.clean = function(deleteValue) {
            for (var i = 0; i < this.length; i++) {
                if (this[i] == deleteValue) {
                    this.splice(i, 1);
                    i--;
                }
            }
            return this;
        };
        
        var contents = [];
        $('.js-group-content').each(function(i, groupContent){
            var contentInsides = $(groupContent).text();
            contents.push(contentInsides);
            return contents;
        });
        
        contents.clean("");
        
        $('.js-group-content-area').each(function(i, groupContentArea){
            $(groupContentArea).text(contents[i]);
        });
        
        
    </script>
    
    